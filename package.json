{
  "name": "object-to-object-mapper",
  "description": "Map an object to another object with reusable, composable functions",
  "version": "1.0.4",
  "homepage": "https://github.com/suddi/node-object-to-object-mapper",
  "author": {
    "name": "Sudharshan Ravindran",
    "email": "suddir@gmail.com",
    "url": "https://github.com/suddi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suddi/node-object-to-object-mapper"
  },
  "bugs": {
    "url": "https://github.com/suddi/node-object-to-object-mapper/issues"
  },
  "license": "MIT",
  "keywords": [
    "object-to-object",
    "object",
    "map",
    "mapping",
    "transform",
    "mapper",
    "compose",
    "json"
  ],
  "engines": {
    "node": ">= 4.5.0",
    "npm": ">= 2.14.7"
  },
  "scripts": {
    "check-coverage": "istanbul check-coverage",
    "coverage": "istanbul cover _mocha -- -R spec",
    "docs": "jsdoc -c ./jsdoc_conf.json",
