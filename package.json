    "publish-docs": "git subtree push --prefix docs origin gh-pages",
    "test": "npm run lint && npm run doctest && npm run coverage && npm run check-coverage"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "eslint": "^3.0.0",
    "eslint-config-suddi": "^3.0.0",
    "husky": "^0.12.0",
    "istanbul": "^0.4.0",
    "jsdoc": "^3.0.0",
    "jsdoctest": "^1.0.0",
    "mocha": "^3.0.0",
    "rewire": "^2.5.2",
    "sinon": "^1.17.6"
  },
  "eslintConfig": {
    "extends": "suddi/backend"
  }
